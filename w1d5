//write a function reverse(arr) to reverse an array

let array = ["a", "b", "c", "d", "e"];

function reverse (arr){
    for(let i = 0; i < arr.length / 2; i++){
        let temp = arr[i];
        arr[i] = arr[arr.length -i -1];
        arr[arr.length -i -1] = temp;
    }
    return arr
}

console.log(reverse(array))